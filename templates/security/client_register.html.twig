<!doctype html>
<html lang="fr">

	<head>
		<!-- Required meta tags -->
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

		<title>
			{% block title %}{% endblock %}
		</title>

		{% block stylesheets %}{% endblock %}
		<!-- Bootstrap CSS -->
		<link
		rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">

		<!-- CSS -->

		<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">

		<link rel="stylesheet" href="{{ asset('css/base.css') }}">
  </head>

 

	<body>


		<div class="wrapper register-color" style="height:100%;">


			<div class="col-sm-12">

        <a href="{{ url('app_index') }}" class="btn btn-outline-secondary1 position-absolute mt-3" role="button" aria-disabled="true"><i class="fas fa-arrow-left"></i></a>
        <img class="mx-auto d-block pt-5" src="{{ asset('images/logo.png') }}" alt="logo Airlock" width="180"/>
{{ form_start(form) }}

			<div class="col-sm-12">
<div class="text-center pt-4 register"><h1>Inscription Client</h1></div>
<div class="p-2">
  <div class="form-group">
    <div class="col-sm-12">
{#       {{ form_row(form.firstName, { 'label': 'FirstName', 'attr': {
                'placeholder': 'FristName ...',
                'class': 'form-control'
              }}) }} #}
    </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-12">
      {{ form_row(form.businessName, { 'label': 'Nom d\'entreprise', 'attr': {
                'placeholder': 'Nom de votre entreprise ...',
                'class': 'form-control'
              }}) }}
    </div>
  </div>
  
  
  <div class="form-group">
    <div class="col-sm-12">
      {{ form_row(form.email, { 'label': 'Adresse email', 'attr': {
                'placeholder': 'Adresse email ...',
                'class': 'form-control'
              }}) }}
    </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-12">
      {{ form_row(form.password, { 'label': ' Mot de passe', 'attr': {
              'placeholder': 'Votre mot de passe',
              'class': 'form-control'
            }}) }}
    </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-12">
          {{ form_row(form.password_validation, { 'label': ' Mot de passe', 'attr': {
              'placeholder': 'Votre mot de passe',
              'class': 'form-control'
            }}) }}

    </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-12">
      {{ form_row(form.phoneNumber, { 'label': ' Numero de telephone', 'attr': {
        'placeholder': '+33...',
        'class': 'form-control'
      }}) }}
    </div>
  </div>
  {# <div class="form-group">
    <div class="col-sm-12">
    </div>
  </div> #}
  <div class="form-group">
    <div class="col-sm-12 text-center">
      <button class="btn btn-login">S'inscrire</button>
    </div>
  </div>
</div>
</div>
</div>

{{ form_end(form) }}
</div>
</div>
</body>
</html>