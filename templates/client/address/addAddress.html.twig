{% extends "base.html.twig" %}

{% block body %}

<h1>Ajouter une adresse</h1>

{{ form_start(formAddress) }}

<div class="form-group">
    <label for"">Addresse</label>
    {{ form_widget(formAddress.streetName) }}
</div>

<div class="form-group">
    <label for"">Code Postal</label>
    {{ form_widget(formAddress.postalCode) }}
</div>

<div class="form-group">
    <label for"">Pays</label>
    {{ form_widget(formAddress.country) }}
</div>

<div class="form-group">
    <label for"">Capacity</label>
    {{ form_widget(formAddress.capacity) }}
</div>

<div class="form-group">
    <label for"">Prix</label>
    {{ form_widget(formAddress.price) }}
</div>

<div class="form-group">
    <label for"">Ouvert</label>
    {{ form_widget(formAddress.isOpen) }}
</div>

<div class="form-group">
    {{ form_widget(formAddress.save) }}
</div>

{{ form_end(formAddress) }}


{% endblock %}